package com.example.game;

public class SceneData {
	MainActivity act;
	private long star;
	private SceneData(){ } //can't default create
	public static SceneData CreateData(MainActivity activity)
	{
		SceneData t = new SceneData();
		t.star = 0L;
		return t;
	}
	public String getStarStr()
	{
		String str;
		try
		{
			str = String.valueOf(star);
		}catch (Exception e)
		{
			str = new String("");
		}
		return str;
	}
	public void addStar(long val)
	{
		star += val;
	}
}
